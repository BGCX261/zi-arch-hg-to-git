<?php

namespace Arch\AdminBundle\Entity;

use Doctrine\ORM\Mapping as ORM;

/**
 * Arch\AdminBundle\Entity\Personal
 */
class Personal
{
    /**
     * @var integer $id
     */
    private $id;

    /**
     * @var integer $departmentId
     */
    private $departmentId;

    /**
     * @var integer $positionId
     */
    private $positionId;

    /**
     * @var string $pib
     */
    private $pib;

    /**
     * @var datetime $birthday
     */
    private $birthday;

    /**
     * @var Arch\AdminBundle\Entity\Department
     */
    private $department;

    /**
     * @var Arch\AdminBundle\Entity\Position
     */
    private $position;


    /**
     * Set id
     *
     * @param integer $id
     */
    public function setId($id)
    {
        $this->id = $id;
    }

    /**
     * Get id
     *
     * @return integer 
     */
    public function getId()
    {
        return $this->id;
    }

    /**
     * Set departmentId
     *
     * @param integer $departmentId
     */
    public function setDepartmentId($departmentId)
    {
        $this->departmentId = $departmentId;
    }

    /**
     * Get departmentId
     *
     * @return integer 
     */
    public function getDepartmentId()
    {
        return $this->departmentId;
    }

    /**
     * Set positionId
     *
     * @param integer $positionId
     */
    public function setPositionId($positionId)
    {
        $this->positionId = $positionId;
    }

    /**
     * Get positionId
     *
     * @return integer 
     */
    public function getPositionId()
    {
        return $this->positionId;
    }

    /**
     * Set pib
     *
     * @param string $pib
     */
    public function setPib($pib)
    {
        $this->pib = $pib;
    }

    /**
     * Get pib
     *
     * @return string 
     */
    public function getPib()
    {
        return $this->pib;
    }

    /**
     * Set birthday
     *
     * @param datetime $birthday
     */
    public function setBirthday($birthday)
    {
        $this->birthday = $birthday;
    }

    /**
     * Get birthday
     *
     * @return datetime 
     */
    public function getBirthday()
    {
        return $this->birthday;
    }

    /**
     * Set department
     *
     * @param Arch\AdminBundle\Entity\Department $department
     */
    public function setDepartment(\Arch\AdminBundle\Entity\Department $department)
    {
        $this->department = $department;
    }

    /**
     * Get department
     *
     * @return Arch\AdminBundle\Entity\Department 
     */
    public function getDepartment()
    {
        return $this->department;
    }

    /**
     * Set position
     *
     * @param Arch\AdminBundle\Entity\Position $position
     */
    public function setPosition(\Arch\AdminBundle\Entity\Position $position)
    {
        $this->position = $position;
    }

    /**
     * Get position
     *
     * @return Arch\AdminBundle\Entity\Position 
     */
    public function getPosition()
    {
        return $this->position;
    }
}