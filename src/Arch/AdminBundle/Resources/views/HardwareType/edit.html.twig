{% extends "::base.html.twig" %}
{% block body %}
<h1>HardwareType edit</h1>

<form action="{{ path('hardwaretype_update', { 'id': entity.id }) }}" method="post" {{ form_enctype(edit_form) }} class="well">
    {{ form_widget(edit_form) }}
    <p>
        <button type="submit">Edit</button>
    </p>
</form>

<ul class="record_actions">
    <li>
        <a href="{{ path('hardwaretype') }}">
            Back to the list
        </a>
    </li>
    <li>
        <form action="{{ path('hardwaretype_delete', { 'id': entity.id }) }}" method="post">
            {{ form_widget(delete_form) }}
            <button type="submit">Delete</button>
        </form>
    </li>
</ul>
{% endblock %}